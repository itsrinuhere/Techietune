  		<servlet>
  	<servlet-name>all</servlet-name>
  	<servlet-class>com.library.search.Search</servlet-class>
  	</servlet>
  		<servlet>
  	<servlet-name>category</servlet-name>
  	<servlet-class>com.library.search.Search</servlet-class>
